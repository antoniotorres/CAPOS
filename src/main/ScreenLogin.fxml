<AnchorPane id="AnchorPane" minHeight="480.0" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.loginController">
   <children>
      <GridPane alignment="CENTER" layoutX="220.0" layoutY="130.0" prefHeight="101.0" prefWidth="200.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="277.0" minWidth="10.0" prefWidth="188.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="%login.lusername" />
            <TextField fx:id="username" GridPane.rowIndex="1" />
            <Label text="%login.lpassword" GridPane.rowIndex="2" />
            <PasswordField fx:id="password" GridPane.rowIndex="3" />
         </children>
      </GridPane>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="264.0" layoutY="240.0" mnemonicParsing="false" onAction="#loginButton" text="%login.blogin" />
      <Label fx:id="lError" layoutX="302.0" layoutY="295.0" textFill="RED" />
      <ImageView fx:id="imgLogo" fitHeight="150.0" fitWidth="128.0" layoutX="256.0" layoutY="300.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../res/inicio.png" />
         </image></ImageView>
      <ImageView fx:id="imgLogoCom" fitHeight="50.0" fitWidth="128.0" layoutX="256.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
